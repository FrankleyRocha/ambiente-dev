{
	"name": "AMBIENTE-DEV",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/home/toolbox/projects",
	"customizations": {
		"vscode": {
			"extensions": [
				"vscjava.vscode-java-pack",
				"vmware.vscode-boot-dev-pack"
			]
		}
	},
	"forwardPorts": [
		2525
	],
	"portsAttributes": {
		"2525": {
			"label": "smtp4dev",
			"onAutoForward": "notify"
		}
	},
	"remoteUser": "toolbox"
}